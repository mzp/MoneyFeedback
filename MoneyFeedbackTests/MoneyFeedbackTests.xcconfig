// MoneyFeedbackTests.xcconfig
// Common build settings for unit test targets

// MARK: - Product configuration
PRODUCT_BUNDLE_IDENTIFIER = jp.mzp.MoneyFeedbackTests
PRODUCT_NAME = $(TARGET_NAME)

// MARK: - Code signing
CODE_SIGN_STYLE = Automatic

// MARK: - Versioning
CURRENT_PROJECT_VERSION = 1
MARKETING_VERSION = 1.0

// MARK: - Info.plist generation
GENERATE_INFOPLIST_FILE = YES

// MARK: - Deployment targets
IPHONEOS_DEPLOYMENT_TARGET = 26.0

// MARK: - SDK and platform support
SDKROOT = auto
SUPPORTED_PLATFORMS = iphoneos iphonesimulator
TARGETED_DEVICE_FAMILY = 1,2,7

// MARK: - String catalogs
STRING_CATALOG_GENERATE_SYMBOLS = NO

// MARK: - Swift configuration
SWIFT_EMIT_LOC_STRINGS = NO
SWIFT_UPCOMING_FEATURE_MEMBER_IMPORT_VISIBILITY = YES
SWIFT_VERSION = 5.0

// MARK: - Test configuration
BUNDLE_LOADER = $(TEST_HOST)
TEST_HOST = $(BUILT_PRODUCTS_DIR)/MoneyFeedback.app/$(BUNDLE_EXECUTABLE_FOLDER_PATH)/MoneyFeedback
